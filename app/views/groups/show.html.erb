
<p>
  <strong>ID:</strong>
  <%= @group.id %>
</p>

<p>
  <strong>Name:</strong>
  <%= @group.name %>
</p>

<p>
  <strong>user_id:</strong>
  <%= @group.user_id %>
</p>

<%= group_icon(@group) %>

<br/>

<% @group_travels.each do |gt| %>
  <li>
    <td><%= gt.user.name %></td> |
    <td><%= gt.user.employee_id %></td> |
    <td><%= gt.purpose %></td> |
    <td><%= gt.source %></td> |
    <td><%= gt.destination %></td> |
    <td><%= gt.distance %></td> |
    <td><%= gt.created_at %></td>
  </li>
<% end %>

<br/>
<br/>

<%= link_to 'Edit', edit_group_path(@group) %> |
<%= link_to 'Back', groups_path %>
