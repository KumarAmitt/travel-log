<%#= link_to 'Back', groups_path %>



<div class="card blue lighten-5">
  <div class="card-content teal-text darken-4">
    <%= group_icon(@group) %>
    <span class="card-title pink-text accent-1"><%= @group.name %></span>
    <p class="travel_show_text"><span class="brown-text darken-4">Created By: </span> <%= @group.user.name %></p>
    <p class="travel_show_text"><span class="brown-text darken-4">Author Employee Id: </span> <%= @group.user.employee_id %> </p>
    <p class="travel_show_text"><span class="brown-text darken-4">Created On: </span> <%= @group.created_at.to_s(:long_ordinal) %></p>
  </div>
  <div class="card-action right-align">
    <%= link_to '<i class="small material-icons green-text darken-4">arrow_back</i>'.html_safe, groups_path %>
    <%= link_to '<i class="small material-icons">mode_edit</i>'.html_safe, edit_group_path(@group) %>
    <%= link_to '<i class="small material-icons red-text accent-3">delete</i>'.html_safe, @group, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
</div>